alias reload!='. ~/.zshrc'
alias zshconfig="emacsclient ~/.zshrc"
alias ohmyzsh="emacsclient ~/.oh-my-zsh"
alias pp="cd ~/dev/projects"
alias matlab="/Applications/MATLAB_R2015a.app/bin/matlab -nosplash -nodesktop"
